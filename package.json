{
  "name": "gif-miniapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "test:smoke": "npm run test && npm run build",
    "analyze": "ANALYZE=true npm run build",
    "build:analyze": "ANALYZE=true npm run build",
    "preview": "npm run build && npm run start",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:build": "npm run type-check && npm run lint && npm run build",
    "security:scan": "npm audit --audit-level=moderate",
    "assets:setup": "node scripts/fetch-assets.js",
    "assets:check": "node scripts/fetch-assets.js"
  },
  "dependencies": {
    "@coinbase/onchainkit": "^0.37.0",
    "@farcaster/frame-sdk": "^0.1.8",
    "@lighthouse-web3/sdk": "^0.4.0",
    "@neynar/nodejs-sdk": "^3.30.0",
    "@supabase/supabase-js": "^2.53.0",
    "@tanstack/react-query": "^5",
    "@upstash/redis": "^1.34.4",
    "@vercel/analytics": "^1.5.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "fabric": "^6.7.1",
    "gif-frames": "^0.2.4",
    "gif.js": "^0.2.0",
    "gifshot": "^0.4.5",
    "gifuct-js": "^2.1.2",
    "next": "^15.4.6",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.3.8",
    "tailwind-merge": "^3.3.1",
    "viem": "^2.27.2",
    "wagmi": "^2.14.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/gif.js": "^0.2.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "^15.4.6",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.8.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "overrides": {
    "@farcaster/frame-sdk": "^0.1.8"
  }
}
